{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "trailing-comma": [true, "never"],
        "quotemark": [true, "single"],
        "space-before-function-paren": [true, "always"],
        "object-curly-spacing": [true, "always"]
    },
    "rulesDirectory": []
}
